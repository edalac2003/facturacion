									<listheader label="Email" align="center" width="25%" />
									<listheader label="Ciudad" align="center" width="20%" />
								</listhead>
							</listbox>
							
						</div>						
					</center>
					<east width="10%" border="none">
						<div align="center">
						
						</div>						
					</east>
				</borderlayout>
			</center>
			<south height="30%" border="none">
			
			</south>
		</borderlayout>
	</window>
</zk>