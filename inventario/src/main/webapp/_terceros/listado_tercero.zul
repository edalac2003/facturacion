<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>
<?page title="Listado Terceros" contentType="text/html;charset=UTF-8"?>
<zk>
<window border="none">
		<borderlayout height="650px">
			<north height="15%" border="none">
				<div align="center">
				</div>				
			</north>
			<center border="none">
				<borderlayout>
					<west width="10%" border="none">
						<div align="center">
							
						</div>						
					</west>
					<center border="none" apply="${ctrlListarTerceros}">
						<div align="center">
							<listbox id="listadoTerceros">
